struct list['a] {
    values: rawptr['a]
    len: uint
    capacity: uint
}

impl Index[list['a], 'a, uint] {
    fn get(self: *list['a], idx: uint) -> 'a? {
        if idx >= self.len {
            return nil
        }

        ptr = self.values + idx
        some(*ptr)
    }

    fn set(self: *list['a], idx: uint, el: 'a) -> 'a? {
        nil
    }
}
