extern wasi fn fd_write(fd: int, iov: IOVec, iov_len: uint, nwritten: *uint) -> uint

struct IOVec {
    base: *u8  // Starting address
    len: uint  // Number of bytes to transfer
}

fn puts(s: string) -> () {
    nwritten = new(uint, 4)
    io = IOVec { base: s.raw_ptr, len: s.len }
    _ = fd_write(1, io, 1, nwritten)
}
