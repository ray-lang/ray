import super::ToStr
import super::sizeof

fn print(v: 'a) -> () where ToStr['a] {
    s = v.to_str()
    nl = "\n"

    chunks = new(string, 2)
    mut ptr = chunks as rawptr[string]
    *ptr = s
    ptr = ptr + 1
    *ptr = nl

    _ = writev_stdout(chunks, 2)
}

fn puts(s: string) -> () {
    _ = writev_stdout(&s, 1)
}
