struct string {
    raw_ptr: *u8
    len: uint
}

trait Int['a] {
    default (int)
}

impl Int[int] {}

trait ToStr['a] {
    fn to_str(self: 'a) -> string
}

impl ToStr[int] {
    fn to_str(self: int) -> string {
        "int"
    }
}

fn use_it(v: 'a) -> string where ToStr['a] {
    v.to_str()
}

fn main() {
    _ = use_it(42)
}
