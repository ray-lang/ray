struct string {
    raw_ptr: *u8
    len: uint
}

trait Int['a] {
    default (int)
}

impl Int[int] {}
impl Int[uint] {}

trait ToStr['a] {
    fn to_str(self: 'a) -> string
}

impl ToStr[int] {
    fn to_str(self: int) -> string {
        "int"
    }
}

fn print(v: 'a) -> () where ToStr['a] {
    s = v.to_str()
}

fn main() {
    print(42)
}
